{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        { "src": "/asset/(.*)", "dest": "/public/asset/$1" },
        { "src": "/css/(.*)", "dest": "/public/css/$1" },
        { "src": "/js/(.*)", "dest": "/public/js/$1" },
        { "src": "/images/blog/(.*)", "dest": "/public/images/blog/$1" },
        { "src": "/storage/(.*)", "dest": "/public/storage/$1" },
        { "src": "/build/(.*)", "dest": "/public/build/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "public": true,
    "buildCommand": "vite build",
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://yourproductionurl.com",
        "APP_KEY": "base64:bhKjf42cCu1KxVxq+6M6yR/gn0TDDFisrd5GEigGOTA=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "ndvjxj.h.filess.io",
        "DB_PORT": "61002",
        "DB_DATABASE": "dbpanti_adjective",
        "DB_USERNAME": "dbpanti_adjective"
    }
}
